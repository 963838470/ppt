<Application x:Class="ui.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <!--默认资源-->
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="ResourceDictionary/windows.xaml"/>
                <ResourceDictionary Source="ResourceDictionary/StatusBar.xaml"/>
                <ResourceDictionary Source="ResourceDictionary/Tab.xaml"/>
                <ResourceDictionary Source="ResourceDictionary/Tabitem.xaml"/>
                <ResourceDictionary Source="ResourceDictionary/Button.xaml"/>
                <ResourceDictionary Source="ResourceDictionary/menu.xaml"/>
                <ResourceDictionary Source="ResourceDictionary/list.xaml"/>


            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
